{
  "name": "lifebit-backend",
  "version": "1.0.0",
  "description": "Backend de la aplicación de gestión de condominios LifeBit, desarrollado con Node.js, Express y PostgreSQL.",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "worker": "node worker.js",
